{
  "name": "makercoin-wallet",
  "version": "1.0.7",
  "description": "Electron Based Wallet for MakerCoin",
  "main": "main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "makewin": "electron-forge make --platform win32",
    "makemac": "electron-forge make --platform darwin",
    "makelinux": "electron-forge make --platform linux",
    "fix": "electron-fix start"
  },
  "repository": "https://github.com/Aki-1020/makercoin-wallet",
  "keywords": [
    "MakerCoin",
    "Wallet",
    "Electron"
  ],
  "author": "Michael Osullivan",
  "license": "CC0-1.0",
  "dependencies": {
    "@octokit/rest": "19.0.4",
    "big.js": "6.2.1",
    "bip39": "3.0.4",
    "bootstrap": "4.2.1",
    "ed25519": "0.0.5",
    "electron-router": "0.5.1",
    "electron-squirrel-startup": "1.0.0",
    "electron-twig": "1.1.1",
    "got": "11.8.5",
    "http": "0.0.1-security",
    "i18n-2": "0.7.3",
    "jquery": "3.6.4",
    "nedb-promises": "6.1.0",
    "makercoin-js": "git+https://github.com/Aki-1020/makercoin-js.git",
    "popper.js": "1.16.1",
    "portfinder": "1.0.32",
    "qrcode": "1.5.1",
    "semver": "7.3.7",
    "v8-compile-cache": "2.3.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "6.0.5",
    "@electron-forge/maker-deb": "6.0.5",
    "@electron-forge/maker-dmg": "6.0.5",
    "@electron-forge/maker-rpm": "6.0.5",
    "@electron-forge/maker-wix": "6.0.5",
    "@electron-forge/maker-zip": "6.0.5",
    "browser-sync": "2.29.0",
    "electron": "7.3.3",
    "electron-fix": "1.1.3",
    "gulp": "4.0.2"
  },
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./icons/mac/icon.icns"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin",
            "linux"
          ]
        },
        {
          "name": "@electron-forge/maker-dmg",
          "config": {
            "icon": "./icons/mac/icon.icns",
            "background": "./static/media/dmg-background.png",
            "format": "ULFO"
          }
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {
            "options": {
              "icon": "./icons/png/1024x1024.png"
            }
          }
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-wix",
          "config": {
            "name": "MakerCoin Wallet",
            "icon": "./icons/win/icon.ico",
            "language": 1033,
            "manufacturer": "MakerCoin Wallet"
          }
        }
      ]
    }
  }
}
